   "source": [
    "ages = [14,12,11,10,8,7,1]\n",
    "sum = 0 \n",
    "for i in ages:\n",
    "    sum = sum + i\n",
    "mean = sum / len(ages)\n",
    "print(mean)"
   ]
  },
