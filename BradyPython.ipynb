   "source": [
    "sum2 = 0\n",
    "for i2 in ages:\n",
    "    sum2 = sum2 + ((i2 - mean) ** 2)\n",
    "variance = sum2 / (len(ages) - 1)\n",
    "print(variance)"
   ]
